import java.util.*;
public class GetSetViewOfKeysFromHashtableExample {
    public static void main(String[] args) {
        Hashtable<Integer,Integer> tb = new Hashtable<>();//create Hashtable object
        tb.put(1,1);//add key value pairs to Hashtable
        tb.put(2,4);
        tb.put(3,3);
        Set set = tb.keySet();
        System.out.println("print approach 1:");
        System.out.println(set.toString());
        System.out.println("print approach 2:");
        Iterator it = set.iterator();
        while(it.hasNext())
        System.out.println(it.next());
    }
}
/*
print approach 1:
[3, 2, 1]
print approach 2:
3
2
1
*/


import java.util.*;
import java.util.stream.*;
public class Solution {
    public static void main(String[] args) {
        ArrayList<String> lst = new ArrayList<>();
        lst.add("abc");
        lst.add("ABC");
        lst.add("bcd");
        Object[] arr = lst.toArray();
        System.out.println(Arrays.toString(arr));
    }
}
//[abc, ABC, bcd]

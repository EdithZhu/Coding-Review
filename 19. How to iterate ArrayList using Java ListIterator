import java.util.*;
public class ArrayListListIteratorExample {
    public static void main(String[] args) {
        ArrayList<Integer> lst = new ArrayList();
        lst.add(1);
        lst.add(2);
        lst.add(3);
        ListIterator it = lst.listIterator();//ListIterator obj
        while(it.hasNext())
        System.out.println(it.next());
    }
}
